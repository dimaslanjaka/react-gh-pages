{
	"editor.formatOnSave": false,
	"editor.codeActionsOnSave": {
		"source.organizeImports": true,
		"source.fixAll": true
	},
	"files.exclude": {
		"**/node_modules": true,
		"**/tmp": true
	},
	"code-runner.clearPreviousOutput": true,
	"code-runner.saveAllFilesBeforeRun": true,
	"code-runner.runInTerminal": true
}